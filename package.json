{
  "name": "quantconnect-filesync",
  "version": "0.0.1",
  "description": "QuantConnect file sync utility",
  "main": "src/index.js",
  "bin": {
    "qc": "lib/index.js",
    "quantconnect": "lib/index.js"
  },
  "repository": "git@github.com:trestrantham/quantconnect-filesync.git",
  "author": "tres@trestrantham.com",
  "license": "MIT",
  "preferGlobal": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "tsc -p .",
    "dev": "nodemon --watch src --exec \"yarn start\" -e ts",
    "lint": "tslint -p .",
    "start": "ts-node ./src/index.ts --files",
    "refresh": "rm -rf ./node_modules ./package-lock.json && yarn"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "12.x",
    "axios": "^0.19.0",
    "chalk": "^3.0.0",
    "commander": "^4.1.0",
    "dotenv": "^8.0.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "husky": "^3.0.5",
    "if-env": "^1.0.4",
    "lint-staged": "^9.2.5",
    "nodemon": "^1.18.6",
    "prettier": "^1.14.3",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.15.0"
  }
}
